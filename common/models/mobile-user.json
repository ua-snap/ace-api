{
  "name": "mobile_user",
  "plural": "MobileUsers",
  "base": "User",
  "strict": "validate",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "trackChanges": true,
  "persistUndefinedAsNull": true,
  "properties": {
    "id": {
      "type": "string",
      "id": true,
      "defaultFn": "guid"
    },
    "groupId": {
      "type": "number"
    }
  },
  "validations": [],
  "relations": {
    "WeatherReports": {
      "type": "hasMany",
      "model": "weatherreport",
      "foreignKey": "userId"
    },
    "weatherreport": {
      "type": "hasMany",
      "model": "weatherreport",
      "foreignKey": "userId"
    },
    "Positions": {
      "type": "hasMany",
      "model": "position",
      "foreignKey": "userId"
    },
    "position": {
      "type": "hasMany",
      "model": "position",
      "foreignKey": "userId"
    },
    "Group": {
      "type": "belongsTo",
      "model": "group",
      "foreignKey": "groupId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__get__WeatherReports" 
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__get__Positions" 
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "checkpoint" 
    }
  ],
  "methods": []
}
