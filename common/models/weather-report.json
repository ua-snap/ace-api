{
  "name": "weather_report",
  "plural": "WeatherReports",
  "base": "PersistedModel",
  "idInjection": true,
    "trackChanges": true,
    "persistUndefinedAsNull": true,
    "strict": "validate",  
  "options": {
    "validateUpsert": true
  },
  "properties": {
      "id": {
          "id": true,
          "type": "string",
          "defaultFn": "guid"
      },
    "userId": {
      "type": "number",
      "required": true
    },
    "positionId": {
      "type": "number",
      "required": true
    },
    "cloudCover": {
      "type": "string"
    },
    "precipitation": {
      "type": "string"
    },
    "visibility": {
      "type": "string"
    },
    "pressureTendency": {
      "type": "string"
    },
    "pressureValue": {
      "type": "number"
    },
    "temperatureValue": {
      "type": "number"
    },
    "temperatureUnits": {
      "type": "string"
    },
    "windValue": {
      "type": "number"
    },
    "windUnits": {
      "type": "string"
    },
    "windDirection": {
      "type": "string"
    },
    "notes": {
      "type": "string"
    },
    "other": {
      "type": "string"
    },
    "attachment": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "MobileUser": {
      "type": "belongsTo",
      "model": "mobile_user",
      "foreignKey": "userId"
    },
    "Position": {
      "type": "belongsTo",
      "model": "position",
      "foreignKey": "positionId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthorized",
      "permission": "DENY"
    }
  ],
  "methods": []
}
