{
  "name": "group",
  "plural": "Groups",
  "base": "PersistedModel",
  "strict": "validate",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "trackChanges": true,
  "persistUndefinedAsNull": true,
  "properties": {
    "id": {
      "type": "string",
      "id": true,
      "defaultFn": "guid"
    },
    "name": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "MobileUsers": {
      "type": "hasMany",
      "model": "mobile_user",
      "foreignKey": "groupId"
    },
    "Positions": {
      "type": "hasMany",
      "model": "position",
      "foreignKey": "groupId",
      "through": "mobile_user",
      "keyThrough": "userId"
    },
    "WeatherReports": {
      "type": "hasMany",
      "model": "weatherreport",
      "foreignKey": "groupId",
      "through": "mobile_user",
      "keyThrough": "userId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "create"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__get__Positions"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__findById__Positions"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__get__WeatherReports"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__findById__WeatherReports"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__get__MobileUsers"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__findById__MobileUsers"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "checkpoint"
    }
  ],
  "methods": []
}
