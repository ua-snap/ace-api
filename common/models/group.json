{
  "name": "group",
  "plural": "Groups",
  "base": "PersistedModel",
  "idInjection": true,
  "trackChanges": true,
  "persistUndefinedAsNull": true,
  "strict": "validate", 
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "id": {
      "id": true,
      "type": "string",
      "defaultFn": "guid"
    },
    "name": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "MobileUsers": {
      "type": "hasMany",
      "model": "mobile_user",
      "foreignKey": "groupId"
    },
    "Positions": {
      "type": "hasMany",
      "model": "position",
      "foreignKey": "groupId",
      "through": "mobile_user",
      "keyThrough": "userId"
    },
    "WeatherReports": {
      "type": "hasMany",
      "model": "weatherreport",
      "foreignKey": "groupId",
      "through": "mobile_user",
      "keyThrough": "userId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthorized",
      "permission": "DENY"
    }
  ],
  "methods": []
}
