      "required": true
    },
    "timestamp": {
      "type": "date",
      "required": true
    },
    "accuracy": {
      "type": "number",
      "postgresql": {
        "dataType": "numeric"
      }
    },
    "altitude": {
      "type": "number",
      "postgresql": {
        "dataType": "numeric"
      }
    },
    "altitudeAccuracy": {
      "type": "number",
      "postgresql": {
        "dataType": "numeric"
      }
    },
    "heading": {
      "type": "number",
      "postgresql": {
        "dataType": "numeric"
      }
    },
    "speed": {
      "type": "number",
      "postgresql": {
        "dataType": "numeric"
      }
    }
  },
  "validations": [],
  "relations": {
    "MobileUser": {
      "type": "belongsTo",
      "model": "mobile_user",
      "foreignKey": "userId"
    }
  },
  "acls": [
    {
      "accessType": "*",
